    "import random\n",
    "\n",
    "def get_word(filename):\n",
    "    with open(filename) as f:\n",
    "        data = [line.strip() for line in f]\n",
    "        \n",
    "    return random.choice(data)\n",
    "\n",
    "print(get_word('words.txt'))"
   ]
  }
